// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sessions.db"
}

model Session {
  id    Int    @id @default(autoincrement())
  key   String @unique
  value String

  @@map("session")
}
